def bool is_prime(int n)
{
    int half_n;
    half_n = n /2 + 1;

    if (n <= 1){
        return false;
    }
    while (half_n > 1){
        if( n % half_n == 0){
            return false;
        }
        half_n = half_n - 1;
    }
    return true;
}

